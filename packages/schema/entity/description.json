{
	"$schema": "http://json-schema.org/draft-07/schema",
	"properties": {
		"identifier": {
			"type": "string"
		},
		"runtime_identifier": {
			"$ref": "../general/identifiers.json#/definitions/entity_identifiers"
		},
		"is_spawnable": {
			"description": "Whether this entity should have a spawn egg.",
			"type": "boolean"
		},
		"is_summonable": {
			"description": "Whether this entity can be summoned with commands.",
			"type": "boolean"
		},
		"is_experimental": {
			"type": "boolean"
		},
		"animations": {
			"patternProperties": {
				".*": {
					"$ref": "../general/serverAnimationRefs.json"
				}
			}
		},
		"scripts": {
			"properties": {
				"animate": {
					"type": "array",
					"items": {
						"anyOf": [
							{
								"type": "string"
							},
							{
								"patternProperties": {
									".*": {
										"$ref": "../molang/embeddedBeta.json"
									}
								}
							}
						]
					}
				}
			}
		}
	}
}
