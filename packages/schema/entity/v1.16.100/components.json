{
	"$schema": "http://json-schema.org/draft-07/schema",
	"allOf": [
		{
			"$ref": "../v1.16.0/components.json"
		},
		{
			"properties": {
				"minecraft:ageable": {
					"properties": {
						"feed_items": {
							"type": "array",
							"items": {
								"properties": {
									"item": {
										"properties": {
											"any_tag": {
												"type": "array",
												"items": {
													"type": "string"
												}
											}
										}
									}
								}
							}
						}
					}
				},
				"minecraft:breedable": {
					"properties": {
						"breed_items": {
							"type": "array",
							"items": {
								"anyOf": [
									{
										"type": "object",
										"properties": {
											"any_tag": {
												"type": "string"
											}
										}
									}
								]
							}
						}
					}
				},
				"minecraft:bribeable": {
					"properties": {
						"bribe_items": {
							"anyOf": [
								{
									"type": "object",
									"properties": {
										"any_tag": {
											"type": "string"
										}
									}
								}
							]
						}
					}
				},
				"minecraft:inside_block_notifier": {
					"description": "Verifies whether the entity is inside any of the listed blocks.",
					"additionalProperties": false,
					"properties": {
						"block_list": {
							"type": "array",
							"items": {
								"properties": {
									"block": {
										"properties": {
											"name": {
												"$ref": "../../general/vanilla/identifiers.json#/definitions/block_identifiers"
											},
											"states": {
												"$ref": "../../general/vanilla/blockState.json"
											}
										}
									},
									"entered_block_event": {
										"type": "string"
									},
									"exited_block_event": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"minecraft:conditional_bandwidth_optimization": {
					"description": "Defines the Conditional Spatial Update Bandwidth Optimizations of this entity.",
					"additionalProperties": false,
					"properties": {
						"conditional_values": {
							"type": "array",
							"items": {
								"properties": {
									"max_dropped_ticks": {
										"type": "number"
									},
									"max_optimized_distance": {
										"type": "number"
									},
									"use_motion_prediction_hints": {
										"type": "boolean"
									},
									"conditional_values": {
										"type": "array",
										"items": {
											"$ref": "../general/filter.json"
										}
									}
								}
							}
						},
						"default_values": {
							"type": "array",
							"items": {
								"properties": {
									"max_dropped_ticks": {
										"type": "number"
									},
									"max_optimized_distance": {
										"type": "number"
									},
									"use_motion_prediction_hints": {
										"type": "boolean"
									}
								}
							}
						}
					}
				}
			}
		}
	]
}
